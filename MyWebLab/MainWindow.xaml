<Window x:Class="MyWebLab.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MyWebLab"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        Title="MainWindow" Closed="Window_Closed"
        Width="574" Height="622"
        mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Web请求控制面板" HorizontalAlignment="Center" Margin="0,10" FontSize="24"/>
        <ListBox Grid.Row="1" x:Name="MainListBox" Style="{StaticResource TaskListBoxStyle}" ItemTemplate="{StaticResource ListItemDataTemplate}">
        </ListBox>
    </Grid>
</Window>
